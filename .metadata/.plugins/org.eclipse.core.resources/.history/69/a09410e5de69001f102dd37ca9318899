package com.example;

public class App {
	
	
	public static void main(String[] args) {
		
		System.out.println("this is java program using the STS");
		

		
		int a1 = 21;
		
		int a2;
		
		a2= 34;
		
		int x, y;
		
		x= 7;
		y=8;
		
		
//		Arthmetic Operator
		
		
		  int result = 8- 7;
		  
		  System.out.println(result);
		  
		  
//		  increment and decrement 
		  
		   int x1 = 5;
		   
		   int c =  x1++;
		    
		   
		   
		   System.out.println(c);
		
		
		   int d = ++x1;
		   
		   System.out.println(d);
		   
		   
//		    moudles operator %
		   
		   
		   int a = 8%5;
		   
		   System.out.println(a);
		   
		   
		   
//		   assignment operator 
		   
		   int b = 6;
		   
		   
		 
		   
		   b += 2;   //  b = b +2;
		   
		   b -= 2;   // b = b-2;
		   
		   
		   
//		   Boolean logic 
		   
		   
		   
		   boolean   result3 = true & true;
		   
//		   bitwise operators 
		   
		   int result4 = 8 & 7;
		   
		   System.out.println(result4);
		   
		   
		   int result5 = 8 | 7;
		   
		   System.out.println(result5);
		   
		   
//		   left shift operator <<
		   
		   int result6 = 8<<1;
		   
		   
//		   short circuit operator 
		   
		   
		     int n = 4;
		     
		     int m = 5;
		     
		    System.out.println(m > n && m++ > 6);
		    
		    System.out.println(m);
		   
		   
		     
		     
				   
				   
		
		
	}

}
